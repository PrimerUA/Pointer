<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <Button
        android:id="@+id/manual_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Manual update" />
    <Button
            android:id="@+id/get_opponents"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="GetOpponentsListResponse test" />
    <Button
            android:id="@+id/set_reg_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Set ger id" />
    <fragment
        android:name="com.primerworldapps.pointer.ui.fragment.ProposalsListFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>